{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "Rust: cargo build - rusty_pass_gen"
		},
		{
			"label": "cargo run",
			"type": "shell",
			"command": "cargo",
			"args": [
				"run",
				"--bin",
				"mybin",
				"--",
				"-L",
				"100"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "cargo test",
			"type": "shell",
			"command": "cargo",
			"args": [
				"t"
			]
		},
		{
			"label": "cargo doc",
			"type": "shell",
			"command": "cargo",
			"args": [
				"doc",
				"--open"
			]
		}
	]
}